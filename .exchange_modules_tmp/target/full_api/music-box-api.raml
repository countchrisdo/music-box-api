#%RAML 1.0 NamedExample
title: Music Box System API
description: |
    This API will fetch songs from the backend systems
mediaType:
    - application/json
version: "1.0"
protocols:
  - HTTP
  - HTTPS
/songs:
  get:
   displayName: songs
   description: This endpoint will get songs
   responses:
    "200":
     body:
      application/json:
       example:
        strict: false
        value:
        "1": Song 1
        "2": Song 2
        "3": Song 3
        "4": Song 4
       type: object
